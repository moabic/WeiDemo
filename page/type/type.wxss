/* page/type/type.wxss */
/* page/list/index.wxss */
page {
    background-color:rgb(233, 227, 227);
}
.newimg {
    width:750rpx;
}
.parent {
    width:600rpx;
    height:200rpx;
    background-color:#fff;
    position: relative;
    bottom:100rpx;
    left:75rpx;
    padding:20rpx 20rpx;
}
.parent text {
    font-size:50rpx;
}
.parent_zhuanfa {
    position: relative;
    left:450rpx;
    bottom:-60rpx;
    width:150rpx;
    display: flex;
    justify-content: space-between;
    margin-top:20rpx;
}
/* 第二块 */
.second-block{
    background-color:#fff;
    margin-top:30rpx;
    padding:30rpx 0;
}
.second-top {
    display: flex;
    align-items: center;
}
.second-log {
    width:60rpx;
    height:60rpx;
    margin-left:20rpx;
    border-radius: 50%;
}
.second-title {
    margin-left:30rpx;
    letter-spacing: 3rpx;
}
.second-logo {
    width:750rpx;
    margin-top:30rpx;
}
.second-content {
    display: flex;
    justify-content: center;
}
.second-content-title {
   padding:30rpx 20rpx;

}
.second-bottom {
    margin-top:20rpx;
    display: flex;
    justify-content: space-between;
    padding:10rpx 20rpx;
}


.second-right-wrapper {
    width:250rpx;
    display: flex;
    justify-content: space-between;
    margin-top:20rpx;
}
.second-right-wrapper image ,.parent image{
    width:40rpx;
    height:40rpx;
    margin-right: 20rpx;
}
.second-right-wrapper .yinyong{
    position: relative;
    bottom:-5rpx;
}
.second-right-wrapper button::after,.parent_zhuanfa button::after{
    border:none;
}
.second-right-wrapper button,.parent_zhuanfa button {
    display: flex;
    width:70rpx;
    height:40rpx;
    background-color:transparent;
    padding:0px;
    position: relative;
    bottom:5rpx;
}

/* 第三块 */
